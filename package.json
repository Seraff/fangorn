{
  "name": "fangorn",
  "version": "0.1.2-alpha",
  "description": "",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/Seraff/fangorn"
  },
  "scripts": {
    "start": "electron .",
    "test": "./node_modules/mocha/bin/mocha test/*_test.js",
    "start-no-gpu": "electron . --disable-gpu --force-cpu-draw",
    "standard": "standard",
    "pack": "electron-builder --dir",
    "dist": "electron-builder -mwl",
    "dist-mac": "electron-builder -m"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "electron": "^9.0.0",
    "electron-builder": "^21.2.0",
    "mocha": "^7.2.0",
    "spectron": "^11.0.0",
    "spectron-menu-addon": "^0.4.1",
    "standard": "^14.3.4",
    "xml2js": "0.4.19"
  },
  "keywords": [
    "phylogeny",
    "bioinformatics"
  ],
  "build": {
    "appId": "com.kolisko-lab.fangorn",
    "fileAssociations": [
      {
        "ext": [
          "tre",
          "tree",
          "nexus",
          "nex",
          "nxs",
          "newick"
        ],
        "name": "Tree file",
        "role": "Editor"
      }
    ]
  },
  "author": "Serafim Nenarokov",
  "license": "ISC",
  "dependencies": {
    "a-color-picker": "^1.2.1",
    "bio-parsers": "^6.0.4",
    "electron-in-page-search": "^1.3.2",
    "electron-unhandled": "^3.0.2",
    "file-dialog": "0.0.7",
    "jquery": "^3.5.1",
    "mousetrap": "^1.6.5",
    "pako": "^1.0.11",
    "path-data-polyfill": "^1.0.3",
    "save-svg-as-png": "^1.4.17",
    "split.js": "^1.5.11",
    "underscore": "^1.10.2",
    "ve-range-utils": "^2.5.8",
    "ve-sequence-utils": "^3.3.54"
  }
}
